module dls.protocol.messages.other;

import dls.protocol.interfaces.other;

@("$")
void setTraceNotification(SetTraceParams params)
{
    import dls.protocol.logger : logger;

    logger.info("Setting trace level to: %s", params.value);
    logger.trace = params.value;
}
